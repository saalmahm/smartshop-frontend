    "react-hook-form": "^7.71.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/forms": "^0.5.11",
    "@testing-library/jest-dom": "^6.9.1",
